@model List<OrderMaster>

<table class="table table-responsive">
    <tr>
        <th>Order ID</th>
        <th>Customer ID</th>
        <th>Ordered Date</th>
        <th>Ordered Amount</th>
    </tr>
    @foreach (var order in Model)
    {
        <tr>
            <td>@order.OrderId</td>
            <td>@order.CustomerID</td>
            <td>@order.OrderedDate</td>
            <td>@order.OrderedAmount</td>
        </tr>
    }
</table>
